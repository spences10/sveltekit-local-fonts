    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "4.0.0",
    "postcss": "8.4.19",
    "postcss-load-config": "4.0.1",
    "prettier": "2.8.0",
    "prettier-plugin-svelte": "2.8.1",
    "svelte": "3.54.0",
    "svelte-check": "2.10.2",
